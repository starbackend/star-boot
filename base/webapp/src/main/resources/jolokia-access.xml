<?xml version="1.0" encoding="utf-8"?>
<restrict>

	<commands>
		<command>read</command>
		<command>list</command>
		<command>version</command>
		<command>search</command>
	</commands>

	<allow>
		<mbean>
			<name>*:*</name>
			<operation>*List*</operation>
			<operation>get*</operation>
			<operation>list*</operation>
			<operation>dump*</operation>
		</mbean>
	</allow>

</restrict>